{
  "name": "@onelabs/wallet",
  "version": "1.0.0",
  "description": "One chain wallet for zklogin js sdk",
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./lib/index.cjs.js": "./lib/index.browser.cjs.js",
    "./lib/index.esm.js": "./lib/index.browser.esm.js"
  },
  "react-native": "lib/index.native.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "files": [
    "/lib",
    "/src"
  ],
  "scripts": {
    "test": "mocha test/*.test.js",
    "generate": "node scripts/parse_swagger.js",
    "compile:docs": "typedoc --treatWarningsAsErrors",
    "compile:js": "cross-env NODE_ENV=production rollup -c",
    "compile:typedefs": "./scripts/typegen.sh",
    "clean": "rimraf ./doc ./declarations ./lib",
    "dev": "cross-env NODE_ENV=development rollup -c --watch",
    "publish-packages": "semantic-release --repository-url git@https://gitlab.myhhhhh.com/hchainapps/onechain-sdk-js.git",
    "test:prettier": "prettier --check '{,{src,test}/**/}*.{j,t}s'",
    "test:prettier:fix": "pnpm prettier --write '{,{src,test}/**/}*.{j,t}s'",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "mocha './test/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/one-chain-labs/ts-wallet.git"
  },
  "keywords": [
    "onechain",
    "zklogin",
    "wallet"
  ],
  "author": "drukenx",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@noble/curves": "^1.4.2",
    "@noble/hashes": "^1.4.0",
    "@types/lodash": "^4.17.16",
    "@types/node-rsa": "^1.1.4",
    "axios": "^1.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.9.3",
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/config-conventional": "^19.2.2",
    "@onelabs/bcs": "^1.6.0",
    "@onelabs/sui": "^1.26.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/babel__core": "^7.20.5",
    "@types/bn.js": "^5.1.5",
    "@types/bs58": "^4.0.1",
    "@types/chai-as-promised": "^8.0.0",
    "@types/express-serve-static-core": "^5.0.0",
    "@types/mocha": "^10.0.6",
    "@types/mz": "^2.7.4",
    "@types/node-fetch": "2",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "bitcore-mnemonic": "^10.8.0",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "commitlint": "^19.3.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "esm": "^3.2.25",
    "form-data": "^4.0.2",
    "lodash": "^4.17.21",
    "mocha": "^11.0.1",
    "mockttp": "^3.15.0",
    "mz": "^2.7.0",
    "node-abort-controller": "^3.0.1",
    "node-rsa": "^1.1.1",
    "prettier": "^3.3",
    "rimraf": "6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "semantic-release": "^24.0.0",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "start-server-and-test": "^2.0.4",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typedoc": "^0.27.0",
    "typescript": "^5.5.4"
  }
}
